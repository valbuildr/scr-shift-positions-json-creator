		"flowbite-svelte-icons": "^2.1.1",
		"gh-pages": "^6.3.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
